msgstr "person’s name and comment from the dossier"

#: ../source/genetec_notification.rst:13
msgid "степень схожести лиц (уверенность алгоритма в совпадении)"
msgstr "matching confidence"

#: ../source/genetec_notification.rst:14
msgid "имя списка наблюдения"
msgstr "watch list’s name"

#: ../source/genetec_notification.rst:15
msgid "полный кадр"
msgstr "full frame"

#: ../source/genetec_notification.rst:17
msgid "|genetec_notification_en|"
msgstr "|genetec_notification_en|"

